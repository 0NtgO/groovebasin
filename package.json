{
    "name": "groovebasin",
    "description": "No-nonsense music client and daemon based on mpd",
    "author": "Andrew Kelley <superjoe30@gmail.com>",
    "version": "0.0.1",
    "licenses": [{
        "type": "MIT",
        "url": "https://raw.github.com/superjoe30/groovebasin/master/LICENSE"
    }],
    "engines": {
        "node": ">=0.6.8"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/superjoe30/groovebasin.git"
    },
    "dependencies": {
        "socket.io": ">=0.8.7",
        "node-static": ">=0.5.9",
        "coffee-script": ">=1.2.0i",
        "handlebars": ">=1.0.4beta",
        "formidable": ">=1.0.8",
        "stylus": ">=0.24.0"
    },
    "scripts": {
        "install": "make"
    },
    "config": {
      "user_id": "mpd",
      "log_level": 2,
      "http_port": 16242,
      "mpd_host": "localhost",
      "mpd_port": 6600,
      "mpd_conf": "/etc/mpd.conf"
    }
}
