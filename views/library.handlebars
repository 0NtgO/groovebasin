{{#if artists}}
  <ul id="library-items">
    {{#artists}}
      <li>
        <div class="artist hoverable expandable">
          <div class="ui-icon ui-icon-triangle-1-e"></div>
          <span>{{name}}</span>
        </div>
        <ul style="display: none;"></ul>
      </li>
    {{/artists}}
  </ul>
{{else}}
  Empty library
{{/if}}

